#!/usr/bin/env node

/**
 * Module dependencies.
 */

var app = require('../app');
var debug = require('debug')('backend:server');
var http = require('http');

/**
 * Get port from environment and store in Express.
 */

var express = require('express');
var app = express();
var port =  5000;

/**
 * Create HTTP server.
 */

app.listen(port);


